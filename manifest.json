{
  "manifest_version": 3,
  "name": "DM Retailers",
  "version": "1.0",
  "description": "DM Retailers",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "input"
  ],
  "host_permissions": [
    "https://*/*"
  ],
  "icons": {
    "16": "icons/icon1-16.png",
    "32": "icons/icon1-32.png",
    "64": "icons/icon1-64.png",
    "128": "icons/icon1-128.png"
  }
}
